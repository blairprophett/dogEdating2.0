<%= form_for @dog, :html => {:multipart => true} do |f| %>
  <%= f.text_field :name, placeholder: 'Name' %><br/>
  <%= f.text_field :breed, placeholder: 'Breed' %><br/>
  <%= f.text_field :age, placeholder: 'Age' %><br/>
  <%#= select("dog", "park_id", Park.pluck(:name, :id), {:prompt => 'Home Park'}) %>
  <%= f.hidden_field :user_id, :value=>params[:user_id] %>
  <%= f.file_field :image, placeholder: 'Image' %><br/>
  <%= f.submit nil, :class => 'btn btn-primary' %><br/>
<% end %>